{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noImplicitThis": false,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "app/*"
      ],
      "components/*": [
        "app/components/*"
      ],
      "constant/*": [
        "app/constant/*"
      ],
      "i18n/*": [
        "app/i18n/*"
      ],
      "interfaces/*": [
        "app/interfaces/*"
      ],
      "navigation/*": [
        "app/navigation/*"
      ],
      "screens/*": [
        "app/screens/*"
      ],
      "style/*": [
        "app/style/*"
      ],
      "utils/*": [
        "app/utils/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
