{
  "name": "snatch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write \"**/*.js\"",
    "format:json": "prettier --write \"**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"**/*.ts{,x}\"",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "lint": "eslint index.js app storybook test --fix --ext .js,.ts,.tsx && yarn format",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "prepare": "npm-run-all patch hack:*"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^2.3.1",
    "@ant-design/react-native": "^4.2.0",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.1",
    "@react-native-picker/picker": "^1.16.7",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.7",
    "apisauce": "^2.1.1",
    "expo-font": "^9.2.1",
    "expo-localization": "^10.2.0",
    "i18n-js": "^3.8.0",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "mobx-state-tree": "^5.0.2",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-config": "^1.4.4",
    "react-native-fast-image": "^8.3.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keychain": "^7.0.0",
    "react-native-maps": "^0.28.0",
    "react-native-maps-directions": "SchneiderOr/react-native-maps-directions#master",
    "react-native-permissions": "^3.0.5",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.6.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-ui-lib": "^5.30.0",
    "react-native-unimodules": "^0.14.6",
    "reactotron-mst": "^3.1.3",
    "reactotron-react-native": "^5.0.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-optional-catch-binding": "^7.14.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@storybook/addon-storyshots": "^6.3.7",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.1",
    "@types/ramda": "^0.27.44",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "bufferutil": "^4.0.3",
    "canvas": "^2.8.0",
    "detox": "^18.20.2",
    "detox-recorder": "^1.0.151",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "jest-circus": "^27.1.0",
    "jest-expo": "^42.1.0",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postinstall-prepare": "^2.0.0",
    "prettier": "^2.3.2",
    "react-devtools-core": "^4.17.0",
    "react-native-codegen": "^0.0.7",
    "react-powerplug": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "^3.0.2",
    "solidarity": "3.0.4",
    "typescript": "4.1.3",
    "utf-8-validate": "^5.0.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-navigation|@storybook|@react-native-community|expo-localization|@unimodules)"
    ]
  }
}
